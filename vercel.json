{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "database/**",
      "use": "@vercel/static"
    },
    {
      "src": "logo.jpeg",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server.js"
    },
    {
      "src": "/database/(.*)",
      "dest": "/database/$1"
    },
    {
      "src": "/logo.jpeg",
      "dest": "/logo.jpeg"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
